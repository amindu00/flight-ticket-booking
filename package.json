{
    "name": "flight-booking-monorepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "web": "npm run start --workspace=flight-booking",
        "api": "npm run start --workspace=api",
        "build:web": "npm run build --workspace=flight-booking",
        "build:api": "npm run build --workspace=api",
        "test:web": "npm run test --workspace=flight-booking",
        "test:api": "npm run test --workspace=api"
    }
}